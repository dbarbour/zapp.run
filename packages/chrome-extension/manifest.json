{
  "manifest_version": 3,
  "name": "Open in Zapp",
  "version": "1.0",
  "description": "Adds a button to open the example app of a Flutter package in Zapp",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://pub.dev/*"]
    }
  ]
}
